"<tr><td>Code 39</td><td>53</td><td>151</td></tr> "
"<tr><td>QR (16 caractﾃｨres)</td><td>44</td><td>44</td></tr> "
"<tr><td>QR mobile (16 caractﾃｨres)</td><td>44</td><td>44</td></tr> "
"<tr><td>Code 128</td><td>91</td><td>284</td></tr> "
"<tr><td>GS1-128 (UCC/EAN-128)</td><td>91</td><td>345</td></tr> "
"<tr><td>UPC A</td><td>91</td><td>206</td></tr> "
"<tr><td>UPC E</td><td>85</td><td>113</td></tr> "
"<tr><td>EAN 13</td><td>85</td><td>191</td></tr> "
"<tr><td>EAN 8</td><td>78</td><td>127</td></tr> "
"</table>"

